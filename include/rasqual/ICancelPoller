#if !defined(__ICANCELPOLLER__)
#define __ICANCELPOLLER__

#include  "grfexceptions.h"

class ICancelPoller
{
public:
	// Override and throw GPCancelOperationException to abort current operation
	virtual void operator()()
	{
	}
};
#endif  // !defined(__ICANCELPOLLER__)
